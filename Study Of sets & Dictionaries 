        "# Set Operation\n",
        "A = {1, 2, 3, 4, 5}\n",
        "B = {4, 5, 6, 7, 8}\n",
        "print(\"Union :\", A | B)\n",
        "print(\"Intersection :\", A & B)\n",
        "print(\"Difference :\", A - B)\n",
        "print(\"Symmetric Difference\", A ^ B)"
      ],
      "metadata": {
